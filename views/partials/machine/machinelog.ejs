<div id="machineLogInner" class="col-content-box elevation-2">

    <h4>Machine Log</h4>
    <hr>

    <div class="row">
        <div class="log-text-container col-xs-8 col-sm-8 col-lg-9">
            <textarea class="form-control log-text"><%= model.get('machine').log %></textarea>

        </div>
        <div class="log-button-container col-xs-4 col-sm-4 col-lg-3 text-right">
            <div class="form-group">
                <button type="button" class="log-save-button btn btn-info" disabled>Save</button>
            </div>
        </div>
    </div>

</div>